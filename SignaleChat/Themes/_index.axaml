<Styles xmlns="https://github.com/avaloniaui" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  Add Resources Here  -->
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <MergeResourceInclude x:Key="Dark" Source="avares://SignaleChat/Themes/Dark/Dark.axaml" />
                <MergeResourceInclude x:Key="Light" Source="avares://SignaleChat/Themes/Light/Light.axaml" />
            </ResourceDictionary.ThemeDictionaries>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="avares://SignaleChat/Controls/Controls.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
</Styles>